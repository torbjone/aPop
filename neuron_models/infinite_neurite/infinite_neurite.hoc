proc celldef() {
  topol()
  subsets()
}

create dend[1]

proc topol() { local i
  basic_shape()
}
proc basic_shape() {
  dend {pt3dclear() pt3dadd(0, 0, 0, 5) pt3dadd(0, 0, 1000, 5)}
}

objref all
proc subsets() { local i
  objref all
  all = new SectionList()
  dend all.append()
}
access dend
celldef()
