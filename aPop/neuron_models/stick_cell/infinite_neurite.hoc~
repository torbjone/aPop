proc celldef() {
  topol()
  subsets()
}

create axon[1]

proc topol() { local i
  basic_shape()
}
proc basic_shape() {
  axon {pt3dclear() pt3dadd(0, 0, 0, 2) pt3dadd(0, 0, 1000, 2)}
}

objref all
proc subsets() { local i
  objref all
  all = new SectionList()
  axon all.append()
}
access axon
celldef()
